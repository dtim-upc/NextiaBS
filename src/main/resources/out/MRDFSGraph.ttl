@prefix json:             <https://www.essi.upc.edu/dtim/json-metamodel#> .
@prefix nextiaDataSource: <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource/> .
@prefix nextiaSchema:     <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource/Schema/55245/> .
@prefix rdf:              <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:             <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:              <http://www.w3.org/2001/XMLSchema#> .

nextiaSchema:localidad
        rdf:type     rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "localidad" ;
        rdfs:range   xsd:string .

nextiaSchema:calle  rdf:type  rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "calle" ;
        rdfs:range   xsd:string .

nextiaDataSource:55245
        rdf:type                  <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource> ;
        rdfs:label                "concesionario" ;
        nextiaDataSource:format   "JSON" ;
        nextiaDataSource:id       "55245" ;
        nextiaDataSource:path     "src/main/resources/systematic_testing/concesionario.json" ;
        nextiaDataSource:wrapper  "SELECT calle,localidad,provincia,vehiculos_view FROM concesionario LATERAL VIEW explode(vehiculos) AS vehiculos_view_explode_1\nLATERAL VIEW explode(vehiculos_view_explode_1) AS vehiculos_view\n" .

nextiaSchema:provincia
        rdf:type     rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "provincia" ;
        rdfs:range   xsd:string .

nextiaSchema:vehiculos
        rdf:type     rdfs:ContainerMembershipProperty , rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "has vehiculos" ;
        rdfs:range   xsd:string .

nextiaSchema:Object_1
        rdf:type    rdfs:Class ;
        rdfs:label  "concesionario" .
