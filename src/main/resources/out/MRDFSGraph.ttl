@prefix json:             <https://www.essi.upc.edu/dtim/json-metamodel#> .
@prefix nextiaDataSource: <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource/> .
@prefix nextiaSchema:     <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource/Schema/05/> .
@prefix rdf:              <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:             <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:              <http://www.w3.org/2001/XMLSchema#> .

nextiaSchema:title  rdf:type  rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "title" ;
        rdfs:range   xsd:string .

nextiaSchema:creator_Array_1_multidimensional
        rdf:type     rdfs:ContainerMembershipProperty , rdf:Property ;
        rdfs:domain  nextiaSchema:Object_2 ;
        rdfs:label   "has creator_Array_1_multidimensional" ;
        rdfs:range   xsd:int .

nextiaSchema:Object_2
        rdf:type    rdfs:Class ;
        rdfs:label  "creator_Array_1" .

nextiaSchema:creator_Array_1_nationality
        rdf:type     rdf:Property ;
        rdfs:domain  nextiaSchema:Object_2 ;
        rdfs:label   "creator_Array_1_nationality" ;
        rdfs:range   xsd:string .

nextiaSchema:Object_3
        rdf:type    rdfs:Class ;
        rdfs:label  "creator_Array_1_complexStuff_Array_1" .

nextiaSchema:creator_Array_1_complexStuff_Array_1_k
        rdf:type     rdf:Property ;
        rdfs:domain  nextiaSchema:Object_3 ;
        rdfs:label   "creator_Array_1_complexStuff_Array_1_k" ;
        rdfs:range   xsd:int .

nextiaDataSource:05  rdf:type     <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource> ;
        rdfs:label                "arrayTest2" ;
        nextiaDataSource:format   "JSON" ;
        nextiaDataSource:id       "05" ;
        nextiaDataSource:path     "src/main/resources/systematic_testing/arrayTest2.json" ;
        nextiaDataSource:wrapper  "SELECT complexStuff.k, creator_view.full_name AS creator_full_name, multidimensional, creator_view.nationality AS creator_nationality, title, web_url FROM arrayTest2 LATERAL VIEW explode(creator) AS creator_view LATERAL VIEW explode(creator_view.multidimensional) AS multidimensional_explode1 LATERAL VIEW explode(multidimensional_explode1) AS multidimensional LATERAL VIEW explode(creator_view.complexStuff) AS complexStuff_explode_1 LATERAL VIEW explode(complexStuff_explode_1) AS complexStuff_explode_2 LATERAL VIEW explode(complexStuff_explode_2) AS complexStuff" .

nextiaSchema:creator  rdf:type  rdfs:ContainerMembershipProperty , rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "has creator" ;
        rdfs:range   nextiaSchema:Object_2 .

nextiaSchema:creator_Array_1_full_name
        rdf:type     rdf:Property ;
        rdfs:domain  nextiaSchema:Object_2 ;
        rdfs:label   "creator_Array_1_full_name" ;
        rdfs:range   xsd:string .

nextiaSchema:Object_1
        rdf:type    rdfs:Class ;
        rdfs:label  "arrayTest2" .

nextiaSchema:web_url  rdf:type  rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "web_url" ;
        rdfs:range   xsd:string .

nextiaSchema:creator_Array_1_complexStuff
        rdf:type     rdfs:ContainerMembershipProperty , rdf:Property ;
        rdfs:domain  nextiaSchema:Object_2 ;
        rdfs:label   "has creator_Array_1_complexStuff" ;
        rdfs:range   nextiaSchema:Object_3 .
