@prefix json:             <https://www.essi.upc.edu/dtim/json-metamodel#> .
@prefix nextiaDataSource: <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource/> .
@prefix nextiaSchema:     <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource/Schema/1234/> .
@prefix rdf:              <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:             <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:              <http://www.w3.org/2001/XMLSchema#> .

nextiaSchema:Object_1
        rdf:type    rdfs:Class ;
        rdfs:label  "dataset1" .

nextiaSchema:index  rdf:type  rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "index" ;
        rdfs:range   xsd:int .

nextiaSchema:id_  rdf:type  rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "id_" ;
        rdfs:range   xsd:string .

nextiaSchema:available
        rdf:type     rdf:Property ;
        rdfs:domain  nextiaSchema:Object_1 ;
        rdfs:label   "available" ;
        rdfs:range   xsd:boolean .

nextiaDataSource:1234
        rdf:type                  <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource> ;
        rdfs:label                "dataset1" ;
        nextiaDataSource:format   "JSON" ;
        nextiaDataSource:id       "1234" ;
        nextiaDataSource:path     "src/main/resources/pruebaDos.json" ;
        nextiaDataSource:wrapper  "SELECT id_,available,index FROM dataset1 " .
