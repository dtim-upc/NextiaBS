@prefix xsd:              <http://www.w3.org/2001/XMLSchema#> .

nextiaSchema:Object_1
        rdf:type     json:Object ;
        rdfs:label   "dataset1" ;
        json:hasKey  nextiaSchema:available , nextiaSchema:index , nextiaSchema:id_ .

nextiaSchema:  json:hasValue  nextiaSchema:Object_1 .

nextiaSchema:index  rdf:type  json:Key ;
        rdfs:label     "index" ;
        json:hasValue  json:Number .

nextiaSchema:id_  rdf:type  json:Key ;
        rdfs:label     "id_" ;
        json:hasValue  json:String .

nextiaSchema:dataset1
        rdf:type  json:Document .

nextiaSchema:available
        rdf:type       json:Key ;
        rdfs:label     "available" ;
        json:hasValue  json:Boolean .

nextiaDataSource:1234
        rdf:type               <http://www.essi.upc.edu/DTIM/NextiaDI/DataSource> ;
        rdfs:label             "dataset1" ;
